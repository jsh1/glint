      "type": "CGPoint",
      "units": "normalized"
    },
    "scale": {
      "displayName": "Scale",
      "type": "CGFloat",
      "sliderMin": 0,
      "sliderMax": 4
    },
    "squeeze": {
      "displayName": "Squeeze",
      "type": "CGFloat",
      "sliderMin": 0,
      "sliderMax": 4
    },
    "skew": {
      "displayName": "Skew",
