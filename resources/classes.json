      "displayName": "Repeated",
      "type": "bool"
    }
  },

  "MgGradientLayer": {
    "colors": {
      "displayName": "Colors",
      "type": "NSArray",
      "subtype": "CGColor"
    },
    "locations": {
      "displayName": "Locations",
      "type": "NSArray",
      "subtype": "CGFloat",
      "units": "normalized"
    },
    "radial": {
      "displayName": "Radial",
      "type": "bool"
    },
    "startPoint": {
      "displayName": "Start Point",
      "type": "CGPoint",
      "units": "pixels"
    },
    "endPoint": {
      "displayName": "End Point",
      "type": "CGPoint",
      "units": "pixels"
    },
    "startRadius": {
      "displayName": "Start Radius",
      "type": "CGFloat",
      "units": "pixels",
      "only_if": "radial"
    },
    "endRadius": {
      "displayName": "End Radius",
      "type": "CGFloat",
      "units": "pixels",
      "only_if": "radial"
    },
    "drawsBeforeStart": {
      "displayName": "Draws Before Start",
      "type": "bool"
    },
    "drawsAfterEnd": {
      "displayName": "Draws After End",
      "type": "bool"
    }
