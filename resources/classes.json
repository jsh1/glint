  "MgPathLayer": {
    "displayName": "Path",
    "path": {
      "type": "CGPath"
    },
    "drawingMode": {
      "displayName": "Drawing Mode",
      "type": "enum",
      "values": ["Fill", "EO Fill", "Stroke", "Fill Stroke", "EO Fill Stroke"]
    },
    "fillColor": {
      "displayName": "Fill Color",
      "type": "CGColor"
    },
    "strokeColor": {
      "displayName": "Stroke Color",
      "type": "CGColor"
    },
    "lineWidth": {
      "displayName": "Line Width",
