    },
    "size": {
      "displayName": "Size",
      "type": "CGSize",
      "units": "pixels"
    },
    "origin": {
      "displayName": "Origin",
      "type": "CGPoint",
      "units": "pixels"
    },
    "bounds": {
      "displayName": "Bounds",
      "type": "CGRect",
      "units": "pixels"
    },
    "anchor": {
