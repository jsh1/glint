    "miterLimit": {
      "displayName": "Miter Limit",
      "type": "CGFloat",
      "min": 0,
      "sliderMax": 20
    },
    "lineCap": {
      "displayName": "Line Cap",
      "type": "enum",
      "values": ["Butt", "Round", "Square"]
    },
    "lineJoin": {
      "displayName": "Line Join",
      "type": "enum",
      "values": ["Miter", "Round", "Bevel"]
