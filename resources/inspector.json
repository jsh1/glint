{
  "MgLayer": {
    "displayName": "Layer",
    "properties": [
      "position", "size", "anchor",
      "rotation", "scale", "squeeze", "skew",
      "alpha", "blendMode"
    ]
  },

  "MgGroupLayer": {
    "displayName": "Group",
    "properties": [
      "group"
    ]
  },

  "MgRectLayer": {
    "displayName": "Rect",
    "properties": [
      "cornerRadius", "drawingMode",
      "fillColor", "strokeColor", "lineWidth"
    ]
  },

  "MgPathLayer": {
    "displayName": "Path",
    "properties": [
      "path", "drawingMode", "fillColor", "strokeColor",
      "lineWidth", "miterLimit", "lineCap", "lineJoin",
      "lineDashPhase", "lineDashPattern"
    ]
  },

  "MgImageLayer": {
    "displayName": "Image",
    "properties": [
      "imageProvider", "repeats", "centerRect", "cropRect"
    ]
  },

  "MgGradientLayer": {
    "displayName": "Gradient",
    "properties": [
      "radial", "colors", "locations",
      "startPoint", "endPoint",
      "startRadius", "endRadius",
      "drawsBeforeStart", "drawsAfterEnd"
    ]
  }
}
